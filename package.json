{
  "name": "free-survey-form-builder",
  "version": "1.0.0",
  "description": "An implementation of survey designer/form builder basing free-survey-core.",
  "repository": "https://github.com/HHaoWang/free-survey-form-builder",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "readme-files"
  ],
  "type": "module",
  "main": "dist/lib/index.js",
  "module": "dist/es/index.js",
  "types": "dist/es/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "default": "./dist/es/index.js",
        "types": "./dist/es/index.d.ts"
      },
      "require": {
        "default": "./dist/lib/index.js",
        "types": "./dist/lib/index.d.ts"
      }
    },
    "./style": {
      "import": "./dist/es/style.css"
    }
  },
  "keywords": [
    "free-survey-core",
    "survey",
    "questionnaire",
    "问卷"
  ],
  "author": "HHao",
  "license": "Apache-2.0",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "dayjs": "^1.11.9",
    "free-survey-core": "^1.3.4",
    "lodash": "^4.17.21",
    "mitt": "^3.0.1",
    "tdesign-icons-vue-next": "^0.2.2",
    "tdesign-vue-next": "^1.9.4",
    "vue": "^3.4.27",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.2",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.12.10",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.25.0",
    "jsdom": "^24.0.0",
    "less": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "typescript": "~5.4.5",
    "vite": "^5.2.11",
    "vite-plugin-build": "^0.10.0",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.16"
  }
}
